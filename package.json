{
  "name": "musix",
  "version": "0.0.0",
  "description": "A unified music service adapter library for TypeScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./spotify": {
      "import": {
        "types": "./dist/spotify.d.ts",
        "default": "./dist/spotify.js"
      },
      "require": {
        "types": "./dist/spotify.d.cts",
        "default": "./dist/spotify.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "check:code": "biome check .",
    "check:types": "tsc --noEmit",
    "fix": "biome check --write .",
    "test": "bun test"
  },
  "keywords": ["music", "spotify", "api", "adapter", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@spotify/web-api-ts-sdk": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.1.14",
    "tsdown": "^0.9.1",
    "typescript": "^5.7.2"
  }
}
